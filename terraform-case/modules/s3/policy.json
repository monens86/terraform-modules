{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:ListBucket"
            ],
            "Principal": "*",
            "Resource": [
                "arn:aws:s3:::${BUCKETNAME}/*",
                "arn:aws:s3:::${BUCKETNAME}"
            ]
        }
    ]
}